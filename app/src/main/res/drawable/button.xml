<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android" >
    <item android:state_pressed="true">
        <shape android:shape="rectangle"  >
            <corners android:radius="150dp" />
            <stroke android:width="1dip" android:color="#5DB92B" />
            <gradient android:angle="-90" android:startColor="#1DBB41" android:endColor="#357A26" />
        </shape>
    </item>
    <item>
        <shape android:shape="rectangle"  >
            <corners android:radius="150dp" />
            <stroke android:width="1dip" android:color="#5DB92B" />
            <gradient android:angle="-90" android:startColor="#1FCF46" android:endColor="#40912E" />
        </shape>
    </item>
</selector>